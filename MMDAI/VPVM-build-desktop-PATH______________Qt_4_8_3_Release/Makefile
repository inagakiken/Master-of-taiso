####### Install

install_libraries: first FORCE
	@$(CHK_DIR_EXISTS) $(INSTALL_ROOT)/lib/ || $(MKDIR) $(INSTALL_ROOT)/lib/ 
	-$(INSTALL_FILE) /usr/lib/i386-linux-gnu/libQtCore.so.4 $(INSTALL_ROOT)/lib/
	-$(INSTALL_FILE) /usr/lib/i386-linux-gnu/libQtGui.so.4 $(INSTALL_ROOT)/lib/
	-$(INSTALL_FILE) /usr/lib/i386-linux-gnu/libQtOpenGL.so.4 $(INSTALL_ROOT)/lib/


uninstall_libraries:  FORCE
	-$(DEL_FILE) -r $(INSTALL_ROOT)/lib/libQtCore.so.4 
	 -$(DEL_FILE) -r $(INSTALL_ROOT)/lib/libQtGui.so.4 
	 -$(DEL_FILE) -r $(INSTALL_ROOT)/lib/libQtOpenGL.so.4
	-$(DEL_DIR) $(INSTALL_ROOT)/lib/ 


install_plugins: first FORCE
	@$(CHK_DIR_EXISTS) $(INSTALL_ROOT)/plugins/ || $(MKDIR) $(INSTALL_ROOT)/plugins/ 
	-$(INSTALL_DIR) /usr/lib/i386-linux-gnu/qt4/plugins/accessible $(INSTALL_ROOT)/plugins/
	-$(INSTALL_DIR) /usr/lib/i386-linux-gnu/qt4/plugins/accessiblebridge $(INSTALL_ROOT)/plugins/
	-$(INSTALL_DIR) /usr/lib/i386-linux-gnu/qt4/plugins/bearer $(INSTALL_ROOT)/plugins/
	-$(INSTALL_DIR) /usr/lib/i386-linux-gnu/qt4/plugins/codecs $(INSTALL_ROOT)/plugins/
	-$(INSTALL_DIR) /usr/lib/i386-linux-gnu/qt4/plugins/designer $(INSTALL_ROOT)/plugins/
	-$(INSTALL_DIR) /usr/lib/i386-linux-gnu/qt4/plugins/graphicssystems $(INSTALL_ROOT)/plugins/
	-$(INSTALL_DIR) /usr/lib/i386-linux-gnu/qt4/plugins/iconengines $(INSTALL_ROOT)/plugins/
	-$(INSTALL_DIR) /usr/lib/i386-linux-gnu/qt4/plugins/imageformats $(INSTALL_ROOT)/plugins/
	-$(INSTALL_DIR) /usr/lib/i386-linux-gnu/qt4/plugins/inputmethods $(INSTALL_ROOT)/plugins/
	-$(INSTALL_DIR) /usr/lib/i386-linux-gnu/qt4/plugins/menubar $(INSTALL_ROOT)/plugins/
	-$(INSTALL_DIR) /usr/lib/i386-linux-gnu/qt4/plugins/phonon_backend $(INSTALL_ROOT)/plugins/
	-$(INSTALL_DIR) /usr/lib/i386-linux-gnu/qt4/plugins/qmltooling $(INSTALL_ROOT)/plugins/
	-$(INSTALL_DIR) /usr/lib/i386-linux-gnu/qt4/plugins/script $(INSTALL_ROOT)/plugins/
	-$(INSTALL_DIR) /usr/lib/i386-linux-gnu/qt4/plugins/sqldrivers $(INSTALL_ROOT)/plugins/
	-$(INSTALL_DIR) /usr/lib/i386-linux-gnu/qt4/plugins/systemtrayicon $(INSTALL_ROOT)/plugins/


uninstall_plugins:  FORCE
	-$(DEL_FILE) -r $(INSTALL_ROOT)/plugins/accessible 
	 -$(DEL_FILE) -r $(INSTALL_ROOT)/plugins/accessiblebridge 
	 -$(DEL_FILE) -r $(INSTALL_ROOT)/plugins/bearer 
	 -$(DEL_FILE) -r $(INSTALL_ROOT)/plugins/codecs 
	 -$(DEL_FILE) -r $(INSTALL_ROOT)/plugins/designer 
	 -$(DEL_FILE) -r $(INSTALL_ROOT)/plugins/graphicssystems 
	 -$(DEL_FILE) -r $(INSTALL_ROOT)/plugins/iconengines 
	 -$(DEL_FILE) -r $(INSTALL_ROOT)/plugins/imageformats 
	 -$(DEL_FILE) -r $(INSTALL_ROOT)/plugins/inputmethods 
	 -$(DEL_FILE) -r $(INSTALL_ROOT)/plugins/menubar 
	 -$(DEL_FILE) -r $(INSTALL_ROOT)/plugins/phonon_backend 
	 -$(DEL_FILE) -r $(INSTALL_ROOT)/plugins/qmltooling 
	 -$(DEL_FILE) -r $(INSTALL_ROOT)/plugins/script 
	 -$(DEL_FILE) -r $(INSTALL_ROOT)/plugins/sqldrivers 
	 -$(DEL_FILE) -r $(INSTALL_ROOT)/plugins/systemtrayicon
	-$(DEL_DIR) $(INSTALL_ROOT)/plugins/ 


install_translations: first FORCE
	@$(CHK_DIR_EXISTS) $(INSTALL_ROOT)/locales/ || $(MKDIR) $(INSTALL_ROOT)/locales/ 
	-$(INSTALL_FILE) /usr/share/qt4/translations/qt_ja.qm $(INSTALL_ROOT)/locales/


uninstall_translations:  FORCE
	-$(DEL_FILE) -r $(INSTALL_ROOT)/locales/qt_ja.qm
	-$(DEL_DIR) $(INSTALL_ROOT)/locales/ 


install:  install_libraries install_plugins install_translations  FORCE

uninstall: uninstall_libraries uninstall_plugins uninstall_translations   FORCE

